{
  "name": "@monkish/vendure-paystack-plugin",
  "version": "0.0.1",
  "dependencies": {
    "@vendure/admin-ui-plugin": "^3.1.1",
    "dotenv": "16.4.7",
    "graphql-tag": "*",
    "graphql": "*",
    "@nestjs/graphql": "*"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@vendure/core": "*"
  },
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "typings": "./dist/index.d.ts",
  "private": true,
  "nx": {
    "sourceRoot": "packages/paystack/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/paystack/dist",
          "main": "packages/paystack/src/index.ts",
          "tsConfig": "packages/paystack/tsconfig.lib.json",
          "format": [
            "cjs"
          ],
          "declarationRootDir": "packages/paystack/src"
        }
      },
      "test": {
        "executor": "@nx/vite:test",
        "outputs": [
          "{options.reportsDirectory}"
        ],
        "options": {
          "reportsDirectory": "../../coverage/packages/paystack"
        }
      },
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "ts-node packages/paystack/dev-server/index.ts"
          ]
        }
      }
    },
    "name": "paystack"
  }
}
